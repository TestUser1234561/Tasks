<%=	render partial: 'breadcrumbs' %>

<div class="grid-center fill-x">
    <div class="grid-center destroy-confirmation">
        <h2>Are you sure you want to destroy this task?</h2>
        <div class="destroy-buttons">
            <%= link_to 'Destroy', project_task_path(@project, @task), data: {:confirm => 'Are you sure?'}, :method => :delete %>
